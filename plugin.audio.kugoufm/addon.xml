<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<addon id="plugin.audio.kugoufm"
	name="酷狗电台(KugouFM)"
	version="1.0.1"
	provider-name="keygle,osfans">
	<extension point="xbmc.python.pluginsource" library="default.py">
        <provides>audio</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>kugou fm audio plugin</summary>
        <summary lang="zh">酷狗电台插件</summary>
        <description>kugou fm audio plugin</description>
        <description lang="zh">酷狗电台频道,数十个音乐电台为你精心打造,带你邂逅喜爱的音乐,享受随意听音乐的精彩。</description>
        <platform>all</platform>
    </extension>
</addon>
